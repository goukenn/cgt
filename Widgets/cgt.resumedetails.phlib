		$d->addLabel()->Content = _("Show google maps");
		$i = $d->addXmlNode("input")->setAttributes(array(
			"id"=>$this->get_field_id("showgooglemap"),
			"name"=>$this->get_field_name("showgooglemap"),
			"type"=>"checkbox",
			"value"=>"1"
		));
		if ($show_gmap){
			//$i->activate("checked");
			$i["checked"]=1;
		}


		$sl = null;
		$tab = array();

		foreach($tab as $k=>$v){
			if ($sl===null){
				$d->addLabel()->Content = "Presentation";
				$d->add("select");
				$sl["name"] = $this->get_field_name("presentation");
			}
			$opt = $sl->add("option");
			$opt["value"]=$k;
			$opt->Content= $v;
		}

		$v_n = $d->addDiv();
		$v_n->addLabel()->Content = "itemPerPage";
		$i = $v_n->addXmlNode("input")->setAttributes(array(
			"id"=>$this->get_field_id("itemPerPage"),
			"name"=>$this->get_field_name("itemPerPage"),
			"type"=>"text",
			"value"=>igk_getv($instance, "itemPerPage", "10")
		));

		$v_n = $d->addDiv();
		$v_n->addLabel()->Content = self::SERVERPAGINATION;
		$i = $v_n->addXmlNode("input")->setAttributes(array(
			"id"=>$this->get_field_id(self::SERVERPAGINATION),
			"name"=>$this->get_field_name(self::SERVERPAGINATION),
			"type"=>"checkbox",
			"value"=>"1"
		));

		if ($srv_pagination){
			$i["checked"]=1;
		}


		$v_n = $d->addDiv();
		$v_n->addLabel()->Content = "shuffle";
		$i = $v_n->addXmlNode("input")->setAttributes(array(
			"id"=>$this->get_field_id("shuffle"),
			"name"=>$this->get_field_name("shuffle"),
			"type"=>"checkbox",
			"value"=>"1"
		));

		if ($shuffle){
			//$i->activate("checked");
			$i["checked"]=1;
		}

		$d->RenderAJX();
	}
}

igk_cgt_register_widget("IGK_CGT_ResumeDetailsWidget");
