		// igk_wln("query page ".$pagename);
		//igk_wln("server ".$serverpagination );
		// igk_wln(igk_cgt_base_uri());
		// igk_wln($nav);
		// exit;

		$group = igk_cgt_get_env("cgt://current_group");

		$qsrv =igk_cgt_get_service('query',1);
		$thsrv =igk_cgt_get_service('thesaurus',1);		

		$cq = igk_getv(igk_cgt_get_group(), $group);
		$xml = "";
		$hxslt = "";
		
		// igk_wln($_SERVER);
		// igk_wln($args);
		// igk_wln($instance);
		
		$detailuri = igk_cgt_base_uri()."/details";
		
		// igk_exit();
		$tab = array();
		$qsrv->setPagined($serverpagination);
		$qsrv->setShuffle($shuffle);
		$qsrv->setContent(2);
		$qsrv->setItemPerPage($max);
		
		$filters = igk_getv($instance, "filters") ?? igk_cgt_get_filter();
		
		$bfilter = igk_cgt_get_env("cgt://filterpagequery");
	
		
		
		
		if ($nav){			
			// igk_wln($nav);			
			$qsrv->setToken($nav->token);
			$qsrv->setPage($nav->page);
			$tab  = array(); //ignore filter  
		}
		else{
			if ($bfilter){
				foreach($bfilter as $k=>$v){
					switch($k){
						case IGK_CGT_MCHOICE:						
					
						// igk_wln($v);
						foreach($v as $mchoices){
							// igk_wln("m chocice .... ");
							foreach($mchoices as $mchoice){
								$tab[$mchoice] = igk_cgt_q_op("equal", "true");
							}
						}
						
						break;
					default:
					
					switch(igk_count($v)){
						case 1:
							$tab[$k]= igk_cgt_q_op("equal", $v[0]);
						break;
						default:
							// $tab[$k] = igk_cgt_q_op("in",implode(",", $v));
							$tab[$k] = igk_cgt_q_op("in", $v);
						break;
					}
					
					}
				}
			}
			//$tab["urn:fld:class"] = igk_cgt_q_op("in","urn:val:class:5star");
			
			//igk_wln($tab);
			// igk_wln($bfilter);
			//exit;
		
		}
		//$query = igk_count($tab)>0? igk_cgt_build_query_request($tab, $cq->key) : null;
		// igk_ilog($query->Render());
		// igk_exit();		
		
		if(isset($cq->key) && !empty($cq->key)){
			$query = igk_count($tab)>0? igk_cgt_build_query_request($tab, $cq->key ?? null) : null;
			// $criteria = $query->addXmlNode("CriteriaQuery");
			// $criteria->add("value")->Content = $cq->key;
			if ($query){
				igk_io_w2file("d:\\temp\\catar.xml", $query->Render());
				// igk_wcode($query->Render());
			}
			// igk_exit();		
			$xml = $qsrv->sendKeyRequest($cq->key, $query);		
		}else{
			$xml = "<?xml version=1.0 ?>";
		}
		
		//e7176b0d-df13-4124-8a2a-f71a9a5af2ec
		 // igk_ilog(igk_cgt_get_last_query());
		 // exit;
		
		
		//remove namespce important for xsl minified value
		$xml = str_replace("xmlns=\"http://pivot.tourismewallonie.be/files/xsd/pivot/3.1\"", "", $xml);
		
		if (empty($xml)){
			$n->addPanel("igk-danger")->Content = __("NoResult");
			$n->RenderAJX();
			return;
		}
		
		
		$header = $qsrv->getHeader($xml);		
		$f = IGK_CGT_INSTALLDIR."/Data/xsl/search_resume.xsl";
		if (file_exists($f)){
			$hxslt = igk_io_read_allfile($f);			
		}else{
			$hxslt = $this->_getPresentationXsl();
		}
	
	
	

		// igk_wln($f);
		// igk_wcode($hxslt);

		
		
		// $dv->addDiv()->addCode()->addObData(array(
		// "group"=>$group,
		// "cq"=>$cq,
		// "xml"=>$xml,
		// "query"=>$query
		// ), "NoTagNode");
		
		
		if ($showmap){
			
			
			$styler = "";
			$map = igk_get_setting_service('googlemap',1);
			$f  = $map->getFileSchema($googleschema);
			if (file_exists($f)){
				$styler = ', styles:'.igk_io_read_allfile($f);
			}
			// igk_wln("styler ...".$f);
			// igk_wln($styler);
			// igk_exit();

			
$c = $dv->addDiv()->setClass("no-print")
->setStyle("height:400px; width:100%; position:relative;")
->addGoogleJSMaps(igk_js_minify("{zoom:15,center:{lat:50.850402, lng:4.357879} {$styler}}"))
->setClass("cgt-resume-map")
->setStyle("height:100%");
// igk_show_textarea($c->Render());
// exit;
}
		// igk_io_w2file("d:\\temp\hebergement.xml", $xml);
		// igk_io_w2file("d:\\temp\hebergement.xsl", $hxslt);
		// igk_ilog(igk_xslt_transform($xml,$hxslt));		
		// exit;
		
		
		$dv->addDiv()->setId("resume")->addXslt($xml, $hxslt, 1, "{min:{$min}, max:{$max}, imguri:'{$imguri}', detailuri:'{$detailuri}'}")
->addBalafonJS()->Content = <<<EOf
(function(){ if(typeof(cgt)!='undefined') cgt.map.init();})();
